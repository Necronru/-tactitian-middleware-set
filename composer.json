{
    "name": "necronru/tactitian-middleware-set",
    "description": "Tactitian CommandBus middleware set",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "league/tactician": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^1.0"
    },
    "authors": [
        {
            "name": "Roman Toroshin",
            "email": "necronru@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Necronru\\Tactitian\\Middleware\\": "src/"
        },
        "files": [
            "src/TypeCast/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Necronru\\Tactitian\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "monolog/monolog": "^1.23",
        "symfony/var-dumper": "^3.3"
    },
    "suggest": {
        "monolog/monolog": "Implementation for psr/log"
    }
}
